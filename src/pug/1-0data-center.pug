extends _layout
include includes/_mixins.pug


block title
	title Data Center
block config
	- var main = true
block content
	.header-wrapper
		+contacts-header
		
		.data-center-slider
			.data-center-slider-bg
				.data-center-bg(style='background-image:url("img/data-slide-1.jpg")')
				.data-center-bg(style='background-image:url("img/data-slide-2.jpg")')
				.data-center-bg(style='background-image:url("img/data-slide-1.jpg")')
			.box.box_inner
				.data-center-slider-text
					.data-center-slider__slide(data-slide-text='Электроснабжение')
						.data-center-slider__slide-wrap
							.header-inner.header-inner_row
								.header-inner__title.header-inner__title_col.slider-title-animate
									h1.h1 Система электросбере&shy;жения
								.header-inner__desc.header-inner__desc_col
									p.text-increased.header-inner__text.slider-text-animate Независимое от городских сетей электропитание на базе ДГУ, также по принципу N+1, что превосходит требования Tier III
									.datacenter-characteristics.slider-characteristics-animate
										+datacenter-characteristics('6 620', 'кв.м. площадь ЦОД')
										+datacenter-characteristics('800', 'Серверных стоек')
										+datacenter-characteristics('4', 'Машинных зала')
					.data-center-slider__slide(data-slide-text='Кондиционирование')
						.data-center-slider__slide-wrap
							.header-inner.header-inner_row
								.header-inner__title.header-inner__title_col.slider-title-animate
									h1.h1 Дата-центр
								.header-inner__desc.header-inner__desc_col
									p.text-increased.header-inner__text.slider-text-animate ЦОД нашего уровня имеют собственную территорию, что исключает форс-мажорных ситуаций с окончанием сроков аренды у самого владельца ЦОД
									.datacenter-characteristics.slider-characteristics-animate
										+datacenter-characteristics('35 000', 'Общая проектная площадь кв.м.')
										+datacenter-characteristics('6620', 'Площадь зданий и сооружений кв.м.')
					.data-center-slider__slide(data-slide-text='Безопасность')
						.data-center-slider__slide-wrap
							.header-inner.header-inner_row
								.header-inner__title.header-inner__title_col.slider-title-animate
									h1.h1 Система электросбере&shy;жения
								.header-inner__desc.header-inner__desc_col
									p.text-increased.header-inner__text.slider-text-animate Независимое от городских сетей электропитание на базе ДГУ, также по принципу N+1, что превосходит требования Tier III
									.datacenter-characteristics.slider-characteristics-animate
										+datacenter-characteristics('6 620', 'кв.м. площадь ЦОД')
										+datacenter-characteristics('800', 'Серверных стоек')
										+datacenter-characteristics('4', 'Машинных зала')
				.data-center-slider__nav
					button(type="button",class="slick-prev").data-center-slider__prev
						+icon('arrow-light')
					button(type="button", class="slick-next").data-center-slider__next
						+icon('arrow-light')
				.data-center-slider__dots
	.box
		+breadcrumbs(['Главная', 'Дата-центер'])
	.box
		.content-wrap
			aside.side-bar.js-stick-parent
				.side-bar-nav.js-stick
					a(href="#").side-bar-nav__mob-btn.show-sidebar-menu
						span.link-value ...
						span.link-i-arrow
							+icon('arrow_down')
					+link-navigation(['О Дата-центре', 'Команда', 'Документация', 'Сертификаты', 'Инфраструктура', 'Новости', 'Галерея'])
			.content-inner
				h2.h2.content-inner__title ЦОД обеспечен двумя независимыми оптическими подключениями, что гарантирует доступность каналов связи
				.datacenter-characteristics.datacenter-characteristics_data
					+datacenter-characteristics('99,982%', 'Уровень доступности сервисов')
					+datacenter-characteristics('35000 м²', 'Общая проектная площадь')
					+datacenter-characteristics('6620 м²', 'Площадь зданий и сооружений')
				.content-inner-item
					h3.h3.content-inner-item__title Сертификация
					.sertification-wrap
						a(href="").sertification-link
							.sertification-link__img
								img(src="img/png/sertificat-1.png", alt="PCI DSS")
							p.text-primary.sertification-link__title PCI DSS
						a(href="").sertification-link
							.sertification-link__img
								img(src="img/png/sertificat-2.png", alt="ISO27001")
							p.text-primary.sertification-link__title ISO27001
						a(href="").sertification-link
							.sertification-link__img
								img(src="img/png/sertificat-3.png", alt="Tier III Design")
							p.text-primary.sertification-link__title Tier III Design
						a(href="").sertification-link
							.sertification-link__img
								img(src="img/png/sertificat-4.png", alt="Tier III Facility")
							p.text-primary.sertification-link__title Tier III Facility
				.content-inner-item
					.accordeon-wrapper.accordeon-wrapper_data
						.accordeon-head
							.accordeon-head-title.h4.js-accordeon-trigger
								span Система электроснабжения 
								.accordeon-head-icon
									span
										+icon('arrow_services')
							.accordeon-head-text.text-primary Все активное оборудование дублируется по принципу N+1Энергосистема организована на базе двух независимых вводов как со стороны городских сетей, так и оборудования клиента. Климатическое оборудование также построено на базе резервирования
